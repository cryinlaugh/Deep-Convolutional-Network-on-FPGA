<?xml version="1.0" encoding="UTF-8"?>
<project name ="CNN_FW_Conv_V0_EngineCode_run_script" default="all" basedir="../../EngineCode">
<property environment="env"/>
<condition property="maxcompilerdirpath" value="${env.MAXCOMPILERJCP}">
		<isset property="env.MAXCOMPILERJCP" />
	</condition>
<condition property="maxcompilerdirpath" value="${env.MAXCOMPILERDIR}/lib/MaxCompiler.jar">
		<not>
			<isset property="maxcompilerdirpath" />
		</not>
	</condition>
<condition property="EnvClassPath" value="${env.CLASSPATH}"><isset property="env.CLASSPATH"/></condition>
<target name="all" depends="run"/>
<target name="build">
		<ant inheritAll="false" antfile="../RunRules/DFE/CNN_FW_Conv_V0_Build.xml"/>
	</target>
<target name="run" depends="build">
		<condition property="args" value="">
			<not>
				<isset property="args"/>
			</not>
		</condition>
	<java classname="cnn_fw_conv_v0.CNN_FW_Conv_V0Manager" fork="true" failonerror="true">
			<env key="MAXSOURCEDIRS" value="src"/>
		<jvmarg value="-Xmx2048m"/>
		<arg line="${args}" />
		<classpath>
				<pathelement path="${env.ANT_MAXELER_INTERNAL_MAXIDE_JARS}"/><pathelement path="../RunRules/DFE/dist/bin"/>
		<pathelement path="${maxcompilerdirpath}"/><pathelement path="${EnvClassPath}"/>
			</classpath>
		</java>
	</target>
<target name="clean">
		<ant inheritAll="false" antfile="../RunRules/DFE/CNN_FW_Conv_V0_Build.xml" target="clean"/>
	</target>
<target name="jar" depends="build">
		<mkdir dir="../RunRules/DFE/dist/jar"/>
	<copy includeemptydirs="false" todir="../RunRules/DFE/dist/jar">
			<fileset dir="../RunRules/DFE/dist/bin" excludes="**/*.uad, **/*.ucd, **/*.uld, **/*.upd, **/*.udd, **/*.uod, **/*.usd, **/*.utd, **/*.uud, **/*.odd, **/*.ead, **/*.ecd, **/*.eld, **/*.epd, **/*.edd, **/*.eod, **/*.esd, **/*.etd, **/*.eud, **/*.urd*.uml, **/*.ecore, **/*.launch, **/*.java, **/*.maxj, **/package.html"/>
		</copy>
	<jar destfile="CNN_FW_Conv_V0Manager.jar" basedir="../RunRules/DFE/dist/jar">
			<manifest>
				<attribute name="Main-Class" value="cnn_fw_conv_v0.CNN_FW_Conv_V0Manager"/>
			</manifest>
		</jar>
	</target>
<target name="help">
		<echo message="ant -f CNN_FW_Conv_V0.xml -Dargs=&quot;${Value}&quot;&#9;Run project" />
	<echo message="ant -f CNN_FW_Conv_V0.xml build&#9;&#9;&#9;Compile project" />
	<echo message="ant -f CNN_FW_Conv_V0.xml run -Dargs=&quot;${Value}&quot;&#9;Run project" />
	<echo message="ant -f CNN_FW_Conv_V0.xml clean&#9;&#9;&#9;Delete temp folders" />
	<echo message="ant -f CNN_FW_Conv_V0.xml jar&#9;&#9;&#9;Create jar package" />
	<echo message="ant -f CNN_FW_Conv_V0.xml help&#9;&#9;&#9;Print help message" />
	</target>
</project>
